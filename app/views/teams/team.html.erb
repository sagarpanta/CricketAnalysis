
<h2><%= @lineup[0]['teamname']%></h2>


<table id="teams" cellspacing="0" cellpadding="0" >
  <tr>
	<th>Country</th>
	<th>Name</th>
	<th>Player Type</th>
  </tr>
<% @lineup.each do |player| %>
  <tr>
	<td><%= @lineup[0]['country'] %></td>
    <td><%= player.player_name %></td>
    <td><%= player.player_type %></td>
  </tr>
<% end %>
</table>

<br />
 <div id="newteam"><%= link_to 'Teams', teams_path %> | <%= link_to 'Next (New Match)', new_match_path %></div>

