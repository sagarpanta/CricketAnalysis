		<li class="transparent">h</li>
		<li data-link='players' class="<%= @playerorder %>"><span class="pretty" title="Player"><%=  image_tag('/assets/player.png') %></span></li>
		<li class="transparent">h</li>
		<li data-link='managers' class="<%= @managerorder %>"><span class="pretty" title="Manager"><%=  image_tag('/assets/manager.png') %></span></li>
		<li class="transparent">h</li>
		<li data-link='coaches' class="<%= @coachorder %>"><span class="pretty" title="Coach"><%= image_tag('/assets/coach.png') %></span></li>
