<h3></h3>

<div id="clientid" style="display:none"><%= current_user.id %></div>
<div id="filters"></div>
<table id="reports" cellspacing="0" cellpadding="0" >
  <tr>
	<th width="400px">Report Name</th>
	<th width="80px"></th>
	<th width="80px"></th>
  </tr>

  
<% @reports.each do |report| %>
  <tr class="playerdata">
	<td width="400px"><%= report.reportname %></td>
	<td width="80px" class="view_report" data-id=<%= report.id %>>Show</td>
	<td width="80px"><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>


<div id="reportcontainer" style="width: 800px; max-height: 200px;display:none;"></div>
<div id="charttable" style="display:none"></div>

<div class='back_to_reports' style="display:none">
<%= image_tag '/assets/back.png' %>
</div>



