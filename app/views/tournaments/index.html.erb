<table id="tournaments" cellspacing="0" cellpadding="0" >
  <tr>
    <th>Name</th>
    <th>Date Start</th>
    <th>Date End</th>
    <th>Format</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tournaments.each do |tournament| %>
  <tr>
    <td><%= tournament.name %></td>
    <td><%= tournament.getStartDate %></td>
    <td><%= tournament.getEndDate %></td>
    <td><%= tournament.getFormat %></td>
    <td><%= link_to 'Show', tournament %></td>
    <td><%= link_to 'Edit', edit_tournament_path(tournament) %></td>
    <td><%= link_to 'Destroy', tournament, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
<div id="newtournament"><%= link_to 'New Tournament', new_tournament_path %></div>

