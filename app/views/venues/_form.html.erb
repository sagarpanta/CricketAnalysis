<%= form_for(@venue, :remote=>true) do |f| %>
  <% if @venue.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@venue.errors.count, "error") %> prohibited this venue from being saved:</h2>

      <ul>
      <% @venue.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
   <%= f.number_field :clientkey, :value=>current_user.id, :hidden=>true %>
  </div>  
  <table align="center">
	<tr>
		<td class="caption">Name</td>
		<td class="fields"><%= f.text_field :venuename, :size=>"20" %></td>
	</tr>
	<tr>
		<td class="caption">End One</td>
		<td class="fields"><%= f.text_field :endone, :size=>"20" %></td>
	</tr>
	<tr>
		<td class="caption">End Two</td>
		<td class="fields"><%= f.text_field :endtwo, :size=>"20" %></td>
	</tr>
  </table>
  <div id="success_message"></div>
  <div class="actions">
    <%= f.submit "", :class=>'submit', :type => :image, :src => @img, :data=>@dImg  %>
  </div>
<% end %>
