<table id="countries" cellspacing="0" cellpadding="0" >
  <tr>
    <th>Client</th>
    <th>Country</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @countries.each do |country| %>
  <tr>
    <td><%= country.clientkey %></td>
    <td><%= country.country %></td>
    <td><%= link_to 'Edit', edit_country_path(country) %></td>
    <td><%= link_to 'Destroy', country, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
<div id="newcountry"><%= link_to 'New Country', new_country_path %></div>




