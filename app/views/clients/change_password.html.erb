<div class="x">1</div>
<div id="update_form">
	<%= form_for(:client, :url => changedpassword_path) do |f| %>
	  <div class="changePass">
		New Password<br />
		<%= f.text_field :new %>
	  </div>
	  <div class="changePass">
		Re-enter Password<br />
		<%= f.text_field :confirmation %>
	  </div>
	  <div class="actions" id="submit">
		<%= f.submit "Change Password" %>
	  </div>
	<% end %>
</div>